              providerMetadata: {
                [providerOptionsName]: {
                  itemId: part.id,
                  reasoningEncryptedContent: (_a19 = part.encrypted_content) != null ? _a19 : null
                }
              }
            });
          }
          break;
        }
        case "image_generation_call": {
