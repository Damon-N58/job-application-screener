        if (_issue.format === "regex")
          return `áƒáƒ áƒáƒ¡áƒ¬áƒáƒ áƒ˜ áƒ¡áƒ¢áƒ áƒ˜áƒœáƒ’áƒ˜: áƒ£áƒœáƒ“áƒ áƒ¨áƒ”áƒ”áƒ¡áƒáƒ‘áƒáƒ›áƒ”áƒ‘áƒáƒ“áƒ”áƒ¡ áƒ¨áƒáƒ‘áƒšáƒáƒœáƒ¡ ${_issue.pattern}`;
        return `áƒáƒ áƒáƒ¡áƒ¬áƒáƒ áƒ˜ ${FormatDictionary[_issue.format] ?? issue2.format}`;
      }
      case "not_multiple_of":
